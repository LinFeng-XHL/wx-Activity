.container {
  width: 100%;
  height: 100%;
  
}


.item{
  width:100%;
  height: 80rpx;
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
  margin-bottom: 25rpx;
  display: flex;
  color:#000;
  font-size: 30rpx
}
.item-left{
  width: 30%;
  height: 80rpx;
  text-align: right;
  line-height: 80rpx;
}
.item-right{
  width: 68%;
  height: 80rpx;
  margin-left: 2%;
  text-align: left;
  line-height: 80rpx;
}


.item-m{
  width:100%;
