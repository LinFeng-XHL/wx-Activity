  margin-bottom: 25rpx;
  display: flex;
  color:#000;
  font-size: 30rpx
}

.item-left-m{
  width: 30%;
  height: auto;
  text-align: right;
  line-height: 160rpx;
}
.item-right-m{
  width: 64%;
